        "noImplicitAny": true,
        
        "target": "es5",

        "module": "esnext",

        "strict": true,

        "esModuleInterop": true,

        "skipLibCheck": true,

        "forceConsistentCasingInFileNames": true,

        "outDir": "./dist"

    },
